//plain text
macro p {
    "p" wTag
}
//bold text
macro b {
    "b" wTag
}
//bold header
macro hb {
   "b" tag "h1" tag
    pushHTML
  "h1" cTag "b" cTag
}
//button
macro button {
    "button" tag
    pushHTML
    "button" cTag
}
macro br {
    "br" tag
}
macro img {
    "<img class='img' src='" 
    swap "'style='width:170px;height:140px;'>\n" concat concat
    HTMLOutputFile fAppend
}
macro he {
   "b" tag "h3" tag
    pushHTML
  "h3" cTag "b" cTag
}